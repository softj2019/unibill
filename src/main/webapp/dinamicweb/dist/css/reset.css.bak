

@charset "utf-8";


@font-face {
  font-family:'NanumSquareR';
  font-style:normal;
  src: local(‘NanumSquareR’), local(‘NanumSquareRegular’), local(‘Nanum Square-Regular’);
  src:url('../dist/fonts/NanumSquareR.ttf') format('ttf');
}

@font-face {
  font-family:'NanumSquareB';
  font-style:normal;
  src: local(‘NanumSquareB’), local(‘Nanum SquareB’), local(‘Nanum Square-Bold’);
  src:url('../dist/fonts/NanumSquareB.ttf') format('ttf');
}

@font-face {
  font-family:'NanumSquareEB';
  font-style:normal;
  src: local(‘NanumSquareEB’), local(‘Nanum SquareEB’), local(‘Nanum Square-ExtraBold’);
  src:url('../dist/fonts/NanumSquareEB.ttf') format('ttf');
}


@font-face {
  font-family:'NanumGothic';
  font-style:normal;
  src: local(‘NanumGothic’), local(‘Nanum Gothic-Regular’), local(‘NanumGothic Regular’);
  src:url('../dist/fonts/NanumGothic.woff') format('woff');
}

@font-face {
  font-family:'NanumGothicBold';
  font-style:normal;
  src: local(‘NanumGothicBold’), local(‘NanumGothic-Bold’), local(‘Nanum Gothic Bold’);
  src:url('../dist/fonts/NanumGothicBold.woff') format('woff');
}


